<web-app>

<servlet>
   <servlet-name>mServlet</servlet-name>
   <servlet-class>com.bazar.JServelet</servlet-class>
</servlet>

<servlet-mapping>
   <servlet-name>mServlet</servlet-name>
   <url-pattern>/welcome</url-pattern>
</servlet-mapping>



<filter>
    <filter-name>mFilter</filter-name>
    <filter-class>com.bazar.JFilter</filter-class>
</filter>
<filter-mapping>
    <filter-name>mFilter</filter-name>
    <url-pattern>/welcome</url-pattern>
</filter-mapping>


